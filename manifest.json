{
    "name": "Cookies Transfer",
    "description": "Import and export cookies for a website as a JSON file",
    "version": "1.0",
    "manifest_version": 3,
    "host_permissions": ["https://*/*", "http://*/*"],
    "permissions": ["cookies", "downloads"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}